[product]
application.directory=APP_PATH "/application/" 
application.view.ext="phtml"
application.modules={{default .Env.HI_PHP_MODULES "\"Catalog,Console,Admin,Manufacture\""}}
trace.enabled={{default .Env.HI_PHP_TRACE_ENABLED "false"}}
instrument.enabled={{default .Env.HI_PHP_INSTRUMENT_ENABLED "false"}}
development={{default .Env.HI_PHP_DEVELOPMENT_ENABLED "false"}}
content.generator.enabled={{default .Env.HI_PHP_CONTENT_GENERATOR_ENABLED "true"}}
